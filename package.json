{
  "name": "vantage-rwe",
  "version": "2.0.0",
  "description": "Commercial Intelligence from Real-World Evidence",
  "private": true,
  "scripts": {
    "install:frontend": "cd frontend && npm install",
    "install:backend": "pip install -r requirements.txt",
    "postinstall": "cd frontend && npm install",
    "build": "cd frontend && npm run build",
    "start:backend": "cd backend && python3 -m uvicorn app.main:app --host 0.0.0.0 --port 8000",
    "start:frontend": "cd frontend && PORT=3000 npm start",
    "start:prod": "cd backend && python3 -m uvicorn app.main:app --host 0.0.0.0 --port 8080",
    "start": "npm run start:prod",
    "dev": "concurrently \"npm run start:backend\" \"npm run start:frontend\""
  },
  "dependencies": {
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}

